#. module: l10n_ch_scan_bvr
#: code:addons/l10n_ch_scan_bvr/scan_bvr.py:142
#, python-format
msgid "BVR CheckSum Error, first part"
msgstr "Erreur de contrôle sur le BVR, première partie"

#. module: l10n_ch_scan_bvr
#: code:addons/l10n_ch_scan_bvr/scan_bvr.py:145
#, python-format
