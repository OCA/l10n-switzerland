#. module: l10n_ch_scan_bvr
#: field:scan.bvr,bvr_string:0
msgid "BVR string"
msgstr "Chaîne du BVR"

#. module: l10n_ch_scan_bvr
#: help:scan.bvr,bvr_string:0
msgid "The line at the bottom right of the BVR"
msgstr "La ligne au fond à droite du BVR"

#. module: l10n_ch_scan_bvr
#: field:scan.bvr,journal:0
msgid "Journal"
msgstr "Journal"

#. module: l10n_ch_scan_bvr
#: field:scan.bvr,partner:0
msgid "Partner"
msgstr "Partenaire"

#. module: l10n_ch_scan_bvr
#: help:scan.bvr,partner:0
msgid "The supplier who send the invoice.\n"
"If it is not set you have to create it"
msgstr "Le fournisseur qui envoie la facture.\nLe cas échéant, vous devrez en créer un."

#. module: l10n_ch_scan_bvr
#: field:scan.bvr,account:0
msgid "Partner bank account"
msgstr "Compte financier"

#. module: l10n_ch_scan_bvr
#: help:scan.bvr,account:0
msgid "The bank account of the supplier.\n"
"If it is not set you have to create it"
msgstr "Le compte bancaire du fournisseur.\nLe cas échéant, vous devrez en créer un."

#. module: l10n_ch_scan_bvr
#: field:scan.bvr,currency:0
msgid "Currency"
msgstr "Monnaie"

#. module: l10n_ch_scan_bvr
#: field:scan.bvr,amount:0
msgid "Amount"
msgstr "Montant"

#. module: l10n_ch_scan_bvr
#: field:scan.bvr,product:0
msgid "Related product"
msgstr "Produit y relatif"

#. module: l10n_ch_scan_bvr
#: help:scan.bvr,product:0
msgid "The product related to the supplier.\"
"If set the invoice line will use it automatically."
msgstr "Le produit relatif au fournisseur.\nChoisissez-en un et la ligne de facture le reprendra automatiquement."

#. module: l10n_ch_scan_bvr
#: field:scan.bvr,supplier_invoice_default_product:0
msgid "Default product supplier invoice"
msgstr "Produit par défaut pour facture fournisseur"

#. module: l10n_ch_scan_bvr
#: help:scan.bvr,supplier_invoice_default_product:0
msgid "Used by the scan BVR wizard. If completed, it'll generate\"
" a line with the proper amount and this specified product"
msgstr "Utilisé par l'assistant 'Scan BVR'.\nChoisissez-en un et la ligne de facture le reprendra automatiquement, avec le bon montant."

#. module: l10n_ch_scan_bvr
#: view:scan.bvr:0
msgid "Cancel"
msgstr "Annuler"

#. module: l10n_ch_scan_bvr
#: view:scan.bvr:0
msgid "Validate"
msgstr "Valider"

#. module: l10n_ch_scan_bvr
#: model:ir.actions.act_window,name:ons_productivity_scan_bvr.onsp_wiz_scan_bvr_action
#: model:ir.ui.menu,name:ons_productivity_scan_bvr.onsp_wiz_scan_bvr_menu
msgid "BVR scan"
msgstr "Scan d'un BVR"

#. module: l10n_ch_scan_bvr
