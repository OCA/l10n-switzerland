<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="winbiz_move_line_importer_form" model="ir.ui.view">
        <field name="name">Winbiz move line importer</field>
        <field name="model">account.winbiz.import</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <button
                        name="import_file"
                        type="object"
                        states="draft"
                        string="Import file"
                        class="oe_highlight"
                    />
                    <button
                        name="open_account_moves"
                        type="object"
                        states="done"
                        string="View imported Moves"
                        class="oe_highlight"
                    />
                </header>
                <group>
                    <div>
                        <field name="state" invisible="1"/>
                        <field name="file" class="oe_inline"/>
                    </div>
                </group>
                <group>
                    <field name="company_id"/>
                    <field name="journal_id"/>
                </group>
                <notebook>
                    <page string="Report">
                        <field name="report" nolabel="1" colspan="4"/>
                    </page>
                    <page string="Import help">
                        <field name="help_html"/>
                    </page>
                </notebook>
            </form>
        </field>
    </record>
</odoo>
